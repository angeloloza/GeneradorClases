<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd" version="2.0">
    <package>
        com.cominvi.app.commons.models
    </package>
    <access>
        PROPERTY
    </access>
    <entity class="Empleados">
        <table schema="dbo" name="empleados"/>
        <attributes>
            <id name="idempleado">
                <column name="idempleado" column-definition="bigint identity"/>
            </id>
            <basic name="apellidomaterno">
                <column name="apellidomaterno" nullable="false" length="50"/>
            </basic>
            <basic name="apellidopaterno">
                <column name="apellidopaterno" nullable="false" length="50"/>
            </basic>
            <basic name="estatus">
                <column name="estatus"/>
            </basic>
            <basic name="fechahoraalta">
                <column name="fechahoraalta" column-definition="date" length="10"/>
            </basic>
            <basic name="fechahoramod">
                <column name="fechahoramod" column-definition="datetime"/>
            </basic>
            <basic name="fechanacimiento">
                <column name="fechanacimiento" column-definition="date" length="10"/>
            </basic>
            <basic name="idempleadoalta">
                <column name="idempleadoalta"/>
            </basic>
            <basic name="idempleadomod">
                <column name="idempleadomod"/>
            </basic>
            <basic name="nombre">
                <column name="nombre" nullable="false" length="50"/>
            </basic>
            <basic name="numeronomina">
                <column name="numeronomina"/>
            </basic>
            <basic name="rfc">
                <column name="rfc" nullable="false" length="13"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>